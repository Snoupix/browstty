{
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
